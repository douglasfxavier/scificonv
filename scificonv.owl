    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#attendsTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#attendsTo">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasAttendee"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Attendee"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:comment xml:lang="en">This property sets the relationship (attendance) between a Participant and a Programme Item. Any participant with a minimum of one relationship attendsTo-&gt;ProgrammeItem is inferred then as an Attendee.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Rachel Green attends to film1&apos;: 

(s): &apos;Rachel Green&apos;     (p): attendsTo      (o): film1
                                                
Legend:
(s) subject = asserted individual resource of class Participant, and inferred individual of subclass Attendee
(p) predicate = object property
(o) object = asserted individual resource of class &apos;Film Screening&apos;</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#enrolledOnTrack -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#enrolledOnTrack">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Participant"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Track"/>
        <rdfs:comment xml:lang="en">This property sets the relationship (enrollment) between a Participant and a Track. Any participant should be enrolled on only one of the tracks, which are disjoint.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Stephen Brown is enrolled on Science track&apos;: 

(s): &apos;Stephen Brown&apos;    (p): onTrack    (o): ScienceTrack
                                                
Legend:
(s) subject = asserted individual resource of class Participant
(p) predicate = object property
(o) object = asserted individual resource of class Track</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasAttendee -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasAttendee">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:comment xml:lang="en">It is the inversion property of attendsTo.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Film 1 has the attendee Rachel Green&apos;:  

(s): film1  (p): hasAttendee      (o): &apos;Rachel Green&apos;
                                                
Legend:
(s) subject = asserted individual resource of class &apos;Film Screening&apos;
(p) predicate = inverse object property
(o) object = asserted individual resource of class Participant, and inferred individual of subclass Attendee</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasGender -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasGender">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Participant"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person/Gender"/>
        <rdfs:comment xml:lang="en">It is a functional property that relates a Participant to his/her Gender, which means that each individual must have only one gender: Female or Male, which are disjoint.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the sentence &apos;Maria Silva is a female&apos;:

(s): &apos;Maria Silva&apos;      (p):  hasGender      (o): Female
                                                
Legend:
(s) subject = asserted individual resource of class Participant
(p) predicate =object property
(o) object = asserted individual resource of class Gender</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasGuestOfHonour -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasGuestOfHonour">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isGuestOfHonourOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#GuestOfHonour"/>
        <rdfs:comment xml:lang="en">This property sets the relationship between a Reading and each of the respective individuals of class Participant. Those participants are, therefore, inferred as individuals of class &apos;Guest of honour&apos;.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Reading 1 has as the guest of honour Carl Sagan&apos;:

(s): reading1     (p): hasGuestOfHonour     (o): &apos;Carl Sagan&apos; 
                                                
Legend:
(s) subject = asserted individual resource of class Reading
(p) predicate = object property
(o) object = asserted individual resource of class Participant, and inferred individual of subclass &apos;Guest of honour&apos;</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasInstructor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasInstructor">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isInstructorOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Instructor"/>
        <rdfs:comment xml:lang="en">This property sets the relationship between a Workshop and each of the respective individuals of class Participant. Those participants are, therefore, inferred as individuals of class &apos;Instructor&apos;.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Workshop 1 has as the instructor Neil deGrasse Tyson&apos;:

(s): workshop1     (p): hasInstructor    (o): &apos;Neil deGrasse Tyson&apos; 
                                                
Legend:
(s) subject = asserted individual resource of class Workshop
(p) predicate = object property
(o) object = asserted individual resource of class Participant, and inferred individual of subclass Instructor</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasModerator -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasModerator">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isModeratorOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Moderator"/>
        <rdfs:comment xml:lang="en">This property sets the relationship between a Panel Session and the respective individual of class Participant that is responsible for moderating the panel session. This participant is, therefore, inferred as an individual of class Moderator.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Workshop 1 has as the moderator Ross Geller&apos;:

(s): workshop1     (p): hasModerator    (o): &apos;Ross Geller&apos; 
                                                
Legend:
(s) subject = asserted individual resource of class Workshop
(p) predicate = object property
(o) object = asserted individual resource of class Participant, and inferred individual of subclass Moderator</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasPanellist -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasPanellist">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isPanellistOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Panellist"/>
        <rdfs:comment xml:lang="en">This property sets the relationship of a Panel Session and each of the respective individuals of class Participant that has the panellist role in the given Panel Session. Those participants are, therefore, inferred as individuals of class Panellist.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Panel session 1 has as a panellist Susan Muller&apos;:

(s): panelsession1     (p): hasPanellist    (o): &apos;Susan Muller&apos;
                                                
Legend:
(s) subject = asserted individual resource of class &apos;Panel Session&apos;
(p) predicate = object property
(o) object = asserted individual resource of class Participant, and inferred individual of subclass Panellist</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasProgrammeItem -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasProgrammeItem">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#onTrack"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:comment xml:lang="en">It is the inverse property of onTrack.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Scientific track has the programme item talk 1&apos;:  

(s): ScienceTrack  (p): hasProgrammeItem     (o): talk1
                                                
Legend:
(s) subject = asserted individual resource of class Track
(p) predicate = inverse object property
(o) object = asserted individual resource of class Talk</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasSpeaker -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasSpeaker">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isSpeakerOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Speaker"/>
        <rdfs:comment xml:lang="en">This property sets the relationship of a Talk and the respective individual of class Participant that has the speaker role in the given Talk. This participant is, therefore, inferred as an individual of class Panellist.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the sentence &apos;The talk 1 has as speaker Neil deGrasse Tyson&apos;:

(s): talk1     (p): hasSpeaker     (o):  &apos;Neil deGrasse Tyson&apos;
                                                
Legend:
(s) subject = asserted individual resource of class Talk
(p) predicate = object property
(o) object = asserted individual resource of class Participant, and inferred individual of subclass Speaker</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isGuestOfHonourOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isGuestOfHonourOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:comment xml:lang="en">It is the inverse of hasGuestOfHonour.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Carl Sagan is the guest of honour of reading 1&apos;:

(s): Carl Sagan    (p): isGuestOfHonourOf    (o): reading1
                                                
Legend:
(s) subject = asserted individual resource of class Participant, and inferred individual of subclass &apos;Guest of honour&apos;
(p) predicate = object property
(o) object = asserted individual resource of class Reading</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isInstructorOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isInstructorOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Steve Arnald is instructor of the workshop 1 &apos;:

(s): &apos;Steve Arnald&apos;     (p): isInstructorOf      (o): workshop1
                                                
Legend:
(s) subject = asserted individual resource of class Participant, and inferred individual of subclass Instructor
(p) predicate = object property, inverse of hasInstructor
(o) object = asserted individual resource of class Workshop</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isLocatedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isLocatedIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://vocab.deri.ie/rooms#Floor"/>
                    <rdf:Description rdf:about="http://vocab.deri.ie/rooms#Room"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://vocab.deri.ie/rooms#Building"/>
                    <rdf:Description rdf:about="http://vocab.deri.ie/rooms#Floor"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property sets the transitive relationship between a Room and a Building, through the Floor where the room is located in. Therefore, a room is located on a floor that is located in a building.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the sentence &apos;The floor B1Floor1 is located in the Building 1 Andromeda&apos;:

(s): B1Floor1    (p): inBuilding    (o): &apos;Building 1 Andromeda&apos;
                                             
Legend:
(s) subject = asserted individual resource of class Floor
(p) predicate = object property
(o) object = asserted individual resource of class Building</skos:example>
        <skos:example xml:lang="en">The triple below represents the sentence &apos;The room B1Room101 is located on the first floor of Building 1 Andromeda&apos;:

(s): B1Room1   (p): isLocatedIn    (o):  B1Floor1
                                             
Legend:
(s) subject = asserted individual resource of a subclass of Room
(p) predicate = object property
(o) object = asserted individual resource of class Floor</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isModeratorOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isModeratorOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:comment xml:lang="en">It is inverse of hasModerator.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Susan Muller is moderator of the panel session 1&apos;: 

(s): &apos;Susan Muller&apos;     (p): isModeratorOf      (o): panelsession1
                                                
Legend:
(s) subject = asserted individual resource of class Participant, and inferred individual of subclass Moderator
(p) predicate = object property, inverse of hasModerator
(o) object = asserted individual resource of class &apos;Panel Session&apos;</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isPanellistOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isPanellistOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:comment xml:lang="en">It is inverse of hasPanellist.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Ross Geller is panellist of the panel session 1&apos;: 

(s): &apos;Ross Geller&apos;     (p): isPanellistOf      (o): panelsession1
                                                
Legend:
(s) subject = asserted individual resource of class Participant, and inferred individual of subclass Panellist
(p) predicate = object property, inverse of hasPanellist
(o) object = asserted individual resource of class &apos;Panel Session&apos;</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isSpeakerOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isSpeakerOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <skos:example xml:lang="en">It is inverse of hasPanellist.</skos:example>
        <skos:example xml:lang="en">The triple below represents the sentence &apos;Neil deGrasse Tyson is speaker of the talk 1&apos;:

(s): &apos;Neil deGrasse Tyson&apos;     (p): isSpeakerOf      (o): &apos;talk 1&apos;
                                                
Legend:
(s) subject = asserted individual resource of class Participant, and inferred individual of subclass Speaker
(p) predicate = object property, inverse of hasSpeaker
(o) object = asserted individual resource of class Talk</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#onTrack -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#onTrack">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Track"/>
        <rdfs:comment xml:lang="en">This property sets the relationship between a Programme Item and a Track. Any item should be associated to only one of the tracks, which are disjoint.</rdfs:comment>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Workshop 1 is on Science track&apos;: 

(s): workshop1    (p): onTrack    (o): ScienceTrack
                                                
Legend:
(s) subject = asserted individual resource of class Workshop
(p) predicate = object property
(o) object = asserted individual resource of class Track</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#takesPlaceIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#takesPlaceIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:range rdf:resource="http://vocab.deri.ie/rooms#Room"/>
        <rdfs:comment xml:lang="en">This functional property sets the relationship between a Programme Item and a Room. Any item should be associated to only one of the rooms, which are disjoint.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/title -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/title">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Track"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">It refers to the title of each scheduled Programme Item or Track of the convention.</rdfs:label>
        <rdfs:label xml:lang="en">title</rdfs:label>
        <skos:example xml:lang="en">The triple below represents the asserted sentence &apos;Film1&apos;s title is Interestellar&apos;:  

(s): film1  (p): title     (o): &apos;Interestellar&apos;
                                                
Legend:
(s) subject = asserted individual resource of class &apos;Film Screening&apos;
(p) predicate = data property
(o) object = string</skos:example>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isKeynote -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isKeynote">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Talk"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">According to the Cambridge Dictionary, a keynote is &quot;the most important or most emphasized part of something&quot;. This property refers to talks that are the most relevant on a track or by day.</rdfs:comment>
        <rdfs:label xml:lang="en">is keynote?</rdfs:label>
        <skos:example xml:lang="en">The triple below represents the sentence &apos;The talk 1 is a keynote&apos;:

(s): talk1     (p): &apos;is keynote?&apos;     (o):  TRUE
                                                
Legend:
(s) subject = asserted individual resource of class Talk
(p) predicate = data property
(o) object = boolean</skos:example>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#scheduledDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#scheduledDate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">It refers to the data/time slot of each scheduled Programme Item of the convention.</rdfs:comment>
        <rdfs:label xml:lang="en">scheduled date</rdfs:label>
        <skos:example xml:lang="en">The triple below represents the asserted sentence &apos;Film 1 is on 15th August 2014 at 16:00 &apos;:  

(s): film1  (p): date     (o): &apos;2014-08-15T16:00:00&apos;
                                                
Legend:
(s) subject = asserted individual resource of class &apos;Film Screening&apos;
(p) predicate = data property
(o) object = dateTime</skos:example>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#duration -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2006/time#duration">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">It refers to the total amount of time of each scheduled Programme Item of the convention. It can be expressed in hours or in minutes. See the subclasses and respective commentaries and examples.</rdfs:comment>
        <rdfs:label xml:lang="en">duration</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#hours -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2006/time#hours">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2006/time#duration"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">It refers to the total amount of time, expressed in hours, of each scheduled Programme Item of the convention.</rdfs:comment>
        <rdfs:label xml:lang="en">hours</rdfs:label>
        <skos:example xml:lang="en">The triple below represents the asserted sentence &apos;Film1&apos;s duration in hours is 2&apos;:  

(s): film1  (p): hours     (o): 2
                                                
Legend:
(s) subject = asserted individual resource of class &apos;Film Screening&apos;
(p) predicate = data property
(o) object = decimal</skos:example>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#minutes -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2006/time#minutes">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2006/time#duration"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">It refers to the total amount of time, expressed in minutes, of each scheduled Programme Item of the convention.</rdfs:comment>
        <rdfs:label xml:lang="en">minutes</rdfs:label>
        <skos:example xml:lang="en">The triple below represents the asserted sentence &apos;Film1&apos;s duration in minutes is 120&apos;:  

(s): film1  (p): minutes     (o): 120
                                                
Legend:
(s) subject = asserted individual resource of class &apos;Film Screening&apos;
(p) predicate = data property
(o) object = decimal</skos:example>
    </owl:DatatypeProperty>
    


    <!-- http://xmlns.com/foaf/0.1/name -->

    <owl:DatatypeProperty rdf:about="http://xmlns.com/foaf/0.1/name">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Participant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">As a property imported from the foaf ontology, name refers only to people names.</rdfs:comment>
        <rdfs:label xml:lang="en">name</rdfs:label>
        <skos:example xml:lang="en">The triple below represents the asserted sentence &apos;Person1&apos; name is Carl Sagan&apos;:  

(s): Person1  (p): name     (o): &apos;Carl Sagan&apos;
                                                
Legend:
(s) subject = asserted individual resource of class Participant
(p) predicate = data property
(o) object = string</skos:example>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://vocab.deri.ie/rooms#Building -->

    <owl:Class rdf:about="http://vocab.deri.ie/rooms#Building">
        <ontology:synonym xml:lang="en">Construction</ontology:synonym>
        <rdfs:label xml:lang="en">Building</rdfs:label>
        <skos:definition xml:lang="en">According to the Oxford Dictionary, a building is &apos;a structure with a roof and walls, such as a house or factory&apos;. Buildings may have stacked floors also know as levels.</skos:definition>
        <skos:example xml:lang="en">The triple below represents the sentence &apos;The floor B1Floor1 is located in the Building 1 Andromeda&apos;:

(s): B1Floor1    (p): inBuilding    (o): &apos;Building 1 Andromeda&apos;
                                             
Legend:
(s) subject = asserted individual resource of class Floor
(p) predicate = object property
(o) object = asserted individual resource of class Building</skos:example>
    </owl:Class>
    


    <!-- http://vocab.deri.ie/rooms#Floor -->

    <owl:Class rdf:about="http://vocab.deri.ie/rooms#Floor">
        <ontology:synonym xml:lang="en">Level</ontology:synonym>
        <rdfs:label xml:lang="en">Floor</rdfs:label>
        <skos:definition xml:lang="en">According to the Cambridge Dictionary, it is each level of a building. A Building may have 1 or more instances of Floor.</skos:definition>
        <skos:example xml:lang="en">The triple below represents the sentence &apos;The room B1Room101 is located on the first floor of Building 1 Andromeda&apos;:

(s): B1Room1   (p): isLocatedIn    (o):  B1Floor1
                                             
Legend:
(s) subject = asserted individual resource of subclass Room
(p) predicate = object property
(o) object = asserted individual resource of class Floor</skos:example>
    </owl:Class>
    


    <!-- http://vocab.deri.ie/rooms#Room -->

    <owl:Class rdf:about="http://vocab.deri.ie/rooms#Room">
        <rdfs:label xml:lang="en">Room</rdfs:label>
        <skos:definition xml:lang="en">According to the Cambridge Dictionary, it is &quot;a part of the inside of a building that is separated from other parts by walls, floor, and ceiling&quot;.</skos:definition>
        <skos:definition xml:lang="en">The triple below represents the inferred sentence &apos;Film 1 takes place in the room 305 of Building 1 (auditorium)&apos;: 

(s): film1    (p): takesPlaceIn     (o): &apos;Room 305 of Building 1&apos;
                                                
Legend:
(s) subject = asserted individual resource of class &apos;Film Screening&apos;
(p) predicate = object property
(o) object = asserted individual resource of class Auditorium</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Attendee -->

    <owl:Class rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Attendee">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Participant"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#attendsTo"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#enrolledOnTrack"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Track"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Participant"/>
        <ontology:synonym xml:lang="en">Spectator</ontology:synonym>
        <rdfs:comment xml:lang="en">The attendee is a spectator in the event. Everyone who is not an Instructor, Moderator, Panellist, Speaker or Guest of honour.</rdfs:comment>
        <rdfs:label xml:lang="en">Attendee</rdfs:label>
        <skos:definition xml:lang="en">According to the Cambridge Dictionary, &apos;someone who goes to a place, event, etc.&apos;.</skos:definition>
        <skos:definition xml:lang="en">According to the Oxford Dictionary, it is &apos;a person who attends a conference or other gathering&apos;.</skos:definition>
        <skos:example xml:lang="en">The triple below represents the sentence &apos;Stephen Brown attends to the talk 1&apos;:

(s): &apos;Stepehn Brown&apos;      (p): attendsTo      (o): talk1
                                                
Legend:
(s) subject = asserted individual resource of class Participant, and inferred individual of subclass Attendee
(p) predicate = object property
(o) object = asserted individual resource of class Talk</skos:example>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Auditorium -->

    <owl:Class rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Auditorium">
        <rdfs:subClassOf rdf:resource="http://vocab.deri.ie/rooms#Room"/>
        <rdfs:label xml:lang="en">Auditorium</rdfs:label>
        <skos:definition xml:lang="en">According to the Oxford Dictionary, it is &quot;a large building or hall used for public gatherings, typically speeches or stage performances&quot; (US).</skos:definition>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Film 1 takes place in the room 305 of Building 1 (auditorium)&apos;: 

(s): film1    (p): takesPlaceIn     (o): &apos;Room 305 of Building 1&apos;
                                                
Legend:
(s) subject = asserted individual resource of class &apos;Film Screening&apos;
(p) predicate = object property
(o) object = asserted individual resource of class Auditorium</skos:example>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#EgalitarianPanelSession -->

    <owl:Class rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#EgalitarianPanelSession">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#PanelSession"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasPanellist"/>
                                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                                <owl:onClass rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#FemalePanellist"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#hasPanellist"/>
                                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                                <owl:onClass rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#MalePanellist"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#PanelSession"/>
        <rdfs:label>Egalitarian Panel Session</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#FemalePanellist -->

    <owl:Class rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#FemalePanellist">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Panellist"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#FilmScreening -->

    <owl:Class rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#FilmScreening">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
        <rdfs:label xml:lang="en">Film Screening</rdfs:label>
        <skos:definition xml:lang="en">An entertainment activity within the convention, where the participants can watch sci-fi films in the free time.</skos:definition>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Rachel Green attends to film1&apos;: 

(s): &apos;Rachel Green&apos;     (p): attendsTo      (o): film1
                                                
Legend:
(s) subject = asserted individual resource of class Participant, and inferred individual of subclass Attendee
(p) predicate = object property
(o) object = asserted individual resource of class &apos;Film Screening&apos;</skos:example>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#GuestOfHonour -->

    <owl:Class rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#GuestOfHonour">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Participant"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isGuestOfHonourOf"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Reading"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Participant"/>
        <rdfs:comment xml:lang="en">Only a person can be guest of honour in a ProgrammeItem, and a person is a guest of honour when is related to a ProgrammeItem by the property isGuestOfHonourOf (infered as the inverse of the property hasGuestOfHonour).</rdfs:comment>
        <rdfs:comment xml:lang="en">The only programme items where there are guest of honours are: PanelSession, Reading and Talk.</rdfs:comment>
        <rdfs:label xml:lang="en">Guest of honour</rdfs:label>
        <skos:definition xml:lang="en">According to the Oxford Dictionary, it is &apos;the most important guest at an occasion&apos;.</skos:definition>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Carl Sagan is the guest of honour of reading 1&apos;:

(s): Carl Sagan    (p): isGuestOfHonourOf    (o): reading1
                                                
Legend:
(s) subject = asserted individual resource of class Participant, and inferred individual of subclass &apos;Guest of honour&apos;
(p) predicate = object property
(o) object = asserted individual resource of class Reading</skos:example>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Instructor -->

    <owl:Class rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Instructor">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Participant"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#isInstructorOf"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Workshop"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Participant"/>
        <ontology:synonym xml:lang="en">mentor</ontology:synonym>
        <ontology:synonym xml:lang="en">teacher</ontology:synonym>
        <ontology:synonym xml:lang="en">tutor</ontology:synonym>
        <rdfs:comment xml:lang="en">In the science fiction convention, Instructor is the person who teaches theories and techniques in at least one Workshop. When a person is associated with a Workshop through the object property hasInstructor, it is inferred that this person belongs to the subclass Instructor.</rdfs:comment>
        <rdfs:label xml:lang="en">Instructor</rdfs:label>
        <skos:definition xml:lang="en">According to the Oxford Dictionary, &apos;a person who teaches something&apos;. Instructor is, then, a subclass of Person.</skos:definition>
        <skos:example xml:lang="en">The triple below represents the inferred sentence &apos;Steve Arnald is instructor of the workshop 1 &apos;:

(s): &apos;Steve Arnald&apos;     (p): isInstructorOf      (o): workshop1
                                                
Legend:
(s) subject = asserted individual resource of class Participant, and inferred individual of subclass Instructor
(p) predicate = object property, inverse of hasInstructor
(o) object = asserted individual resource of class Workshop</skos:example>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#LongDurationItem -->

    <owl:Class rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#LongDurationItem">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2006/time#minutes"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">90</xsd:minExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:onDataRange>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#ProgrammeItem"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#MalePanellist -->

    <owl:Class rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#MalePanellist">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#Panellist"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#MeetingRoom -->

    <owl:Class rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#MeetingRoom">
