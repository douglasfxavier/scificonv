<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#"
     xml:base="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv"
     xmlns:schema="https://schema.org/attendee"
     xmlns:rooms="http://vocab.deri.ie/rooms#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:sfc="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:time="http://www.w3.org/2006/time#"
     xmlns:ontology="http://dbpedia.org/ontology/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:po="http://purl.org/ontology/po/">
    <owl:Ontology rdf:about="http://www.semanticweb.org/doug/ontologies/2018/6/scificonv">
        <skos:definition xml:lang="en">Scificonv is an ontology created for describing data of a science fiction convention. It contains the main concepts found in the most popular events from the same category..</skos:definition>
        <rdfs:comment xml:lang="en">Scificonv reuses terms from some well-known ontologies: foaf, dcterms, skos and time.</rdfs:comment>
        <dcterms:licence xml:lang="en">https://creativecommons.org/licenses/by/4.0/</dcterms:licence>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">v.1[beta]</owl:versionInfo>
        <dcterms:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Douglas Xavier</dcterms:creator>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Scificonv Ontology v.1</rdfs:label>
        <rdfs:comment xml:lang="en">Ontology consistency tested with the reasoners FaCT and Pellet</rdfs:comment>
        <rdfs:comment xml:lang="en">Scificonv stands for Science Fiction Convention</rdfs:comment>
        <foaf:mbox>dfxs1n17@soton.ac.uk</foaf:mbox>
    </owl:Ontology>
    


